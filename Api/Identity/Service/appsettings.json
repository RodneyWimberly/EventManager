{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "__DefaultConnection__"
  },
  "IdentityConfig": {
    "IdentityUrl": "__EventManager.Identity.ServiceUrl__",
    "ClientUrl": "__ClientUrl__"
  },
  "UseLocalCertStore": "false",
  "CertificateThumbprint": "",
  "CertificateNameKeyVault": "",
  "AzureKeyVaultEndpoint": "",
  "EmailSettings": {
    "SendGridApiKey": "__SendGridApiKey__",
    "SenderEmailAddress": "__SenderEmailAddress__"
  },
  "fido2": {
    "serverDomain": "localhost",
    "serverName": "IdentityAuthAngular",
    "origin": "https://localhost:44318",
    "timestampDriftTolerance": 300000,
    "MDSAccessKey": null
  }
}