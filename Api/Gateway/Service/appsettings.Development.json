{
  "ReverseProxy": {
    "Routes": [
      {
        "RouteId": "route-connect",
        "BackendId": "backend-connect",
        "Match": {
          "Methods": [ "GET", "POST", "DELETE", "PUT", "PATCH" ],
          "Host": "localhost:6001",
          "Path": "/connect/"
        }
      },
      {
        "RouteId": "route-wellknown",
        "BackendId": "backend-wellknown",
        "Match": {
          "Methods": [ "GET", "POST", "DELETE", "PUT", "PATCH" ],
          "Host": "localhost:6001",
          "Path": "/.well-known/"
        }
      },
      {
        "RouteId": "route-notifications",
        "BackendId": "backend-notifications",
        "Match": {
          "Methods": [ "GET", "POST", "DELETE", "PUT", "PATCH" ],
          "Host": "localhost:6001",
          "Path": "/notifications/"
        }
      },
      {
        "RouteId": "route-events",
        "BackendId": "backend-events",
        "Match": {
          "Methods": [ "GET", "POST", "DELETE", "PUT", "PATCH" ],
          "Host": "localhost:6001",
          "Path": "/events/"
        }
      },
      {
        "RouteId": "route-accounts",
        "BackendId": "backend-accounts",
        "Match": {
          "Methods": [ "GET", "POST", "DELETE", "PUT", "PATCH" ],
          "Host": "localhost:6001",
          "Path": "/accounts/"
        }
      }
    ],
    "Backends": {
      "backend-connect": {
        "Destinations": {
          "backend-connect-destination1": {
            "Address": "https://localhost:6002/connect"
          }
        }
      },
      "backend-wellknown": {
        "Destinations": {
          "backend-wellknown-destination1": {
            "Address": "https://localhost:6002/.well-known"
          }
        }
      },
      "backend-notifications": {
        "Destinations": {
          "backend-notifications-destination1": {
            "Address": "https://localhost:6003/api/notifications"
          }
        }
      },
      "backend-events": {
        "Destinations": {
          "backend-events-destination1": {
            "Address": "https://localhost:6003/api/events"
          }
        }
      },
      "backend-accounts": {
        "Destinations": {
          "backend-accounts-destination1": {
            "Address": "https://localhost:6003/api/accounts"
          }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
