                const msg = `Severity: "${MessageSeverity[severity]}", Summary: "${data}", Detail: "${separatorOrDetail}", Error: "${_helpers_utilities__WEBPACK_IMPORTED_MODULE_4__["Utilities"].safeStringify(error)}"`;
