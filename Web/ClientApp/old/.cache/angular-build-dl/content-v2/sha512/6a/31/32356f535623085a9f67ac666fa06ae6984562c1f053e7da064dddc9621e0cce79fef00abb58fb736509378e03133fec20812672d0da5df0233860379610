        if (index > -1) {
            storedSyncKeys.splice(index, 1);
            this.localStorageSetItem(_helpers_db_keys__WEBPACK_IMPORTED_MODULE_4__["DbKeys"].SYNC_KEYS, storedSyncKeys);
        }
    }
    addToSyncKeysHelper(key) {
