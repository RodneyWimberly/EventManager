module.exports = JSON.parse("{\"app\":{\"Welcome\":\"Bem-vindo!\",\"Notifications\":\"Notificaes\",\"New\":\"Novo\"},\"mainMenu\":{\"Appointments\":\"Compromissos\",\"Customers\":\"Clientes\",\"Products\":\"Produtos\",\"Orders\":\"Pedidos\",\"About\":\"Sobre\",\"Logout\":\"Sair\"},\"pageHeader\":{\"Dashboard\":\"Painel de Controle\",\"Customers\":\"Clientes\",\"Products\":\"Produtos\",\"Orders\":\"Pedidos\",\"NotFound\":\"No Encontrado\",\"About\":\"Sobre\",\"Settings\":\"Configuraes\"},\"home\":{\"NoWidgets1\":\"Voc no possui widgets a serem mostrados. Ir para\",\"NoWidgets2\":\"configurar widgets disponveis\",\"StatisticsTitle\":\"Alguma coisa importante\"},\"notFound\":{\"404\":\"404\",\"pageNotFound\":\"A pgina que voc est procurando no foi encontrada\",\"backToHome\":\"Voltar para a pgina principal\"},\"settings\":{\"tab\":{\"Profile\":\"Perfil\",\"Preferences\":\"Preferncias\",\"Users\":\"Usurios\",\"Roles\":\"Funes\"},\"header\":{\"UserProfile\":\"Perfil de Usurio\",\"UserPreferences\":\"Preferncias de Usurio\",\"UserDepartments\":\"Departamentos de Usurios\",\"UsersManagements\":\"Gerenciamento de Usurios\",\"RolesManagement\":\"Gerenciamento de Funes\"}},\"preferences\":{\"ReloadPreferences\":\"Recarregar Preferncias:\",\"ReloadPreferencesHint\":\"Carregar as preferncias padro (as alteraes atuais sero descartadas)\",\"Language\":\"Idioma:\",\"English\":\"English\",\"French\":\"Franais\",\"German\":\"Deutsche\",\"Portuguese\":\"Portugus\",\"Arabic\":\"\",\"Korean\":\"\",\"LanguageHint\":\"Selecione seu idioma\",\"HomePage\":\"Principal:\",\"Dashboard\":\"Painel de Controle\",\"Customers\":\"Clientes\",\"Products\":\"Produtos\",\"Orders\":\"Pedidos\",\"About\":\"Sobre\",\"Settings\":\"Configuraes\",\"HomePageHint\":\"Selecione a pgina padro para acessar aps o login\",\"Theme\":\"Tema:\",\"DefaultTheme\":\"Padro\",\"CosmoTheme\":\"Cosmo\",\"SolarTheme\":\"Solar\",\"CeruleanTheme\":\"Cerulean\",\"FlatlyTheme\":\"Solitariamente\",\"SketchyTheme\":\"Esboado\",\"S