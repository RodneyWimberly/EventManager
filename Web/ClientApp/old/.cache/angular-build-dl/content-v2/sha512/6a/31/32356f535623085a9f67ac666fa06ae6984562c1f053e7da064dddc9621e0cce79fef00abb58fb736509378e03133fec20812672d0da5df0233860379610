            this.event.eventConditions = this.event.eventConditions.filter(item => item !== row);
        });
    }
    loadData() {
        if (this.event && this.event.eventConditions && this.event.eventConditions.length > 0) {
            this.rows = this.event.eventConditions;
        }
    }
