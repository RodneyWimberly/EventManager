    var AppComponent =
    /*#__PURE__*/
    function () {
      function AppComponent(storageManager, toastaService, toastaConfig, accountClient, alertService, notificationService, appTitleService, authEndpointService, translationService, configurations, router) {
        var _this2 = this;

