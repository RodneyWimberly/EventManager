    function () {
      function EqualValidatorDirective(validateEqual, reverse) {
        _classCallCheck(this, EqualValidatorDirective);

        this.validateEqual = validateEqual;
        this.reverse = reverse;
      }

      _createClass(EqualValidatorDirective, [{
        key: "validate",
        value: function validate(c) {
          var other = c.root.get(this.validateEqual);

          if (!other) {
            return null;
          }

          return this.reverse === 'true' ? this.validateReverse(c, other) : this.validateNoReverse(c, other);
        }
      }, {
        key: "validateNoReverse",
        value: function validateNoReverse(c, other) {
          return other.value === c.value ? null : {
            validateEqual: true
          };
        }
      }, {
        key: "validateReverse",
        value: function validateReverse(c, other) {
          if (c.value === other.value) {
            if (other.errors) {
              delete other.errors.validateEqual;

              if (Object.keys(other.errors).length == 0) {
                other.setErrors(null);
              }
            }
          } else {
            other.setErrors({
              validateEqual: true
            });
          }

          return null;
        }
      }]);

      return EqualValidatorDirective;
